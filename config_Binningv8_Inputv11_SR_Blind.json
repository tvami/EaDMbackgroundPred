{
    "NAME": "EaDM",
    "GLOBAL": {
        "FILE": "EaDM_$process.root",
        "HIST": "h$region",
        "HIST_UP": "h$region_$syst_up",
        "HIST_DOWN": "h$region_$syst_down",
        "path": "./histograms_for_2DAlphabet_v11",
	"SIGNAMEorg": ["Signal_M800GeV_SR","Signal_M900GeV_SR","Signal_M1000GeV_SR","Signal_M1100GeV_SR","Signal_M1200GeV_SR","Signal_M1300GeV_SR","Signal_M1400GeV_SR","Signal_M1500GeV_SR","Signal_M1600GeV_SR","Signal_M1700GeV_SR","Signal_M1800GeV_SR","Signal_M1900GeV_SR","Signal_M2000GeV_SR","Signal_M2100GeV_SR","Signal_M2200GeV_SR","Signal_M2300GeV_SR","Signal_M2400GeV_SR","Signal_M2500GeV_SR","Signal_M2600GeV_SR","Signal_M2700GeV_SR","Signal_M2800GeV_SR","Signal_M2900GeV_SR","Signal_M3000GeV_SR","Signal_M3250GeV_SR","Signal_M3500GeV_SR","Signal_M3750GeV_SR","Signal_M4000GeV_SR","Signal_M4250GeV_SR","Signal_M4500GeV_SR","Signal_M4750GeV_SR","Signal_M5000GeV_SR", "Signal_M90000GeV_SR"],
    	"SIGNAME": ["Signal_M90000GeV_SR"]
    },
    "REGIONS": {
        "pass": {
            "PROCESSES": ["SIGNAME"],
            "BINNING": "default"
        },
        "fail": {
            "PROCESSES": ["SIGNAME"],
            "BINNING": "default"
        }
    },
    "PROCESSES": {
        "data_obs": {
            "SYSTEMATICS":[],
            "COLOR": "black",
            "SCALE": 1,
            "TYPE": "DATA",
            "TITLE": "Cosmics_Data_SR",
            "ALIAS": "Cosmics_Data_SR",
            "LOC": "path/FILE:HIST"
        },
        "SIGNAME": {
            "SYSTEMATICSvolt": ["lumi","pileup", "systOnF", "systOnG", "systOnTrigger", "systOnPt", "systOnMuReco", "systOnMuID", "systOnMuTrigger"],
            "SYSTEMATICS":["lumi","RNN","pT","t0"],
            "COLOR": "red",
            "SCALE": 1.0,
            "TYPE": "SIGNAL",
            "TITLE": "DM signal",
            "LOC": "path/FILE:HIST"
        }
    },
    "SYSTEMATICS": {
        "lumi": {
                "CODE": 0,
                "VAL": 1.05
        },
        "pT": {
            "ALIAS": "pTsyst",
            "UP": "path/FILE:HIST_UP",
            "DOWN": "path/FILE:HIST_DOWN",
            "SIGMA": 1.0
        },
        "t0": {
            "ALIAS": "t0syst",
            "UP": "path/FILE:HIST_UP",
            "DOWN": "path/FILE:HIST_DOWN",
            "SIGMA": 1.0
        },
        "RNN": {
            "ALIAS": "RNNsyst",
            "UP": "path/FILE:HIST_UP",
            "DOWN": "path/FILE:HIST_DOWN",
            "SIGMA": 1.0
	}
    },
    "BINNING": {
        "default": {
            "X": {
                "NAME": "pT",
                "TITLE": "p_{T}",
		"BINSv1":   [0,100,200,300,400,500,600,800,1000,1200,1400,1600,1900,2200,2600,3400,4200,5000],
		"BINSv2":   [0,50,100,150,200,250,300,350,400,450,500,550,600,800,1000,1200,1400,1600,1900,2200,2600,3400,4200,5000],
                "BINSv3":   [0,50,140,270,440,650,900,1190,1520,1890,2300,2750,3240,3770,4340,5000],
                "BINSv6":   [180,320,610,1010,1520,2300,3240,4340,5600],
		"BINS":   [180,320,610,1010,1520,2300,3240,4340,5600,7020],
		"SIGSTART": 1010,
                "SIGEND": 1520,
		"NBINSna": 50,
                "MINna": 500,
                "MAXna": 5500
            },
            "Y": {
                "NAME": "N",
                "TITLE": "N",
                "NBINS": 1,
                "MIN": 1,
                "MAX": 50
            }
        }
    },
    "OPTIONS": {
        "blindedFit": ["pass"],
        "blindedPlots": ["pass"],
        "plotTemplateComparisonsNA": true,
        "plotEvtsPerUnitVolt" : true
    }
}

